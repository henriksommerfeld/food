{
  "name": "recept",
  "description": "Isabels och Henriks receptsamling",
  "version": "1.0.0",
  "author": "Henrik Sommerfeld",
  "keywords": [
    "gatsby",
    "food",
    "recipe"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start": "GATSBY_SCREENSHOT_PLACEHOLDER=true nodemon --exec \"gatsby develop --host=0.0.0.0\"",
    "build": "gatsby build",
    "rebuild": "gatsby clean && gatsby build",
    "develop": "gatsby develop",
    "backend": "npx netlify-cms-proxy-server",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "serve": "gatsby serve --host=0.0.0.0",
    "test:unit": "jest",
    "test:unit:watch": "jest --watchAll --verbose",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:cypress:ci": "start-server-and-test develop http://localhost:8000 test:cypress",
    "cy": "npm run clean && npm run backend & start-server-and-test develop http://localhost:8000 test:cypress:open",
    "cy:cli": "npm run clean && npm run backend & start-server-and-test develop http://localhost:8000 test:cypress",
    "codegen": "graphql-codegen --config graphql-codegen.yml"
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*"
    ],
    "delay": "1500"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/henriksommerfeld/food.git"
  },
  "resolutions": {
    "cypress": "6.2.1"
  },
  "dependencies": {
    "@babel/core": "7.12.10",
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "2.4.2",
    "@raae/gatsby-remark-oembed": "0.1.1",
    "babel-plugin-styled-components": "1.12.0",
    "babel-preset-gatsby": "0.10.0",
    "gatsby": "2.30.2",
    "gatsby-background-image": "1.4.0",
    "gatsby-image": "2.9.0",
    "gatsby-plugin-catch-links": "2.8.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "2.10.0",
    "gatsby-plugin-netlify": "2.9.0",
    "gatsby-plugin-netlify-cache": "1.2.0",
    "gatsby-plugin-netlify-cms": "4.8.0",
    "gatsby-plugin-offline": "3.8.0",
    "gatsby-plugin-react-helmet": "3.8.0",
    "gatsby-plugin-sharp": "2.12.1",
    "gatsby-plugin-styled-components": "3.8.0",
    "gatsby-plugin-typescript": "2.10.0",
    "gatsby-plugin-typography": "2.10.0",
    "gatsby-remark-copy-linked-files": "2.8.0",
    "gatsby-remark-images": "3.9.0",
    "gatsby-remark-relative-images-v2": "0.1.5",
    "gatsby-source-filesystem": "2.9.0",
    "gatsby-transformer-remark": "2.14.0",
    "gatsby-transformer-sharp": "2.10.1",
    "http-proxy-middleware": "1.0.6",
    "identity-obj-proxy": "3.0.0",
    "intersection-observer": "^0.12.0",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.20",
    "lodash-webpack-plugin": "0.11.6",
    "netlify-cms-app": "2.14.12",
    "p-map": "4.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-ripples": "2.2.1",
    "react-spring": "8.0.27",
    "react-typography": "0.16.19",
    "reactn": "2.2.7",
    "remark": "13.0.0",
    "source-map-support": "0.5.19",
    "strip-markdown": "4.0.0",
    "styled-components": "5.2.1",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "typography": "0.16.19",
    "typography-theme-alton": "0.16.19",
    "use-onclickoutside": "0.3.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@testing-library/cypress": "7.0.3",
    "@types/graphql": "14.5.0",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.167",
    "@types/node": "14.14.21",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-helmet": "6.1.0",
    "babel-jest": "26.6.3",
    "cypress": "6.2.1",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.1",
    "start-server-and-test": "1.11.7",
    "ts-jest": "26.4.4"
  }
}
