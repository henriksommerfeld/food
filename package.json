{
  "name": "recept",
  "description": "Isabels och Henriks receptsamling",
  "version": "1.0.0",
  "author": "Henrik Sommerfeld",
  "keywords": [
    "gatsby",
    "food",
    "recipe"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start": "GATSBY_SCREENSHOT_PLACEHOLDER=true nodemon --exec \"gatsby develop --host=0.0.0.0\"",
    "build": "gatsby build",
    "rebuild": "gatsby clean && gatsby build",
    "develop": "gatsby develop",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "serve": "gatsby serve --host=0.0.0.0",
    "format": "prettier --trailing-comma es5 --semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test:unit": "jest",
    "test:unit:watch": "jest --watchAll --verbose",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:cypress:ci": "start-server-and-test develop http://localhost:8000 test:cypress",
    "cy": "npm run clean && start-server-and-test develop http://localhost:8000 test:cypress:open",
    "cy:cli": "npm run clean && start-server-and-test develop http://localhost:8000 test:cypress",
    "type-check": "npx tsc",
    "lint": "eslint . --ext js,ts,jsx,tsx",
    "lint:fix": "npm run lint --fix",
    "pre-commit": "npm run format && npm run lint",
    "codegen": "graphql-codegen --config graphql-codegen.yml",
    "upgrade-packages": "yarn upgrade-interactive && npx syncyarnlock"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*"
    ],
    "delay": "1500"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/henriksommerfeld/food.git"
  },
  "resolutions": {
    "cypress": "4.8.0"
  },
  "dependencies": {
    "@babel/core": "7.10.2",
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "2.4.2",
    "@raae/gatsby-remark-oembed": "0.1.1",
    "babel-plugin-styled-components": "1.10.7",
    "babel-preset-gatsby": "0.4.9",
    "gatsby": "2.23.3",
    "gatsby-background-image": "1.1.1",
    "gatsby-image": "2.4.7",
    "gatsby-plugin-catch-links": "2.3.5",
    "gatsby-plugin-manifest": "2.4.11",
    "gatsby-plugin-netlify": "2.3.5",
    "gatsby-plugin-netlify-cache": "1.2.0",
    "gatsby-plugin-netlify-cms": "4.3.2",
    "gatsby-plugin-offline": "3.2.9",
    "gatsby-plugin-prefetch-google-fonts": "1.4.3",
    "gatsby-plugin-react-helmet": "3.3.4",
    "gatsby-plugin-sharp": "2.6.11",
    "gatsby-plugin-styled-components": "3.3.4",
    "gatsby-plugin-typescript": "2.4.6",
    "gatsby-plugin-typography": "2.5.4",
    "gatsby-remark-copy-linked-files": "2.3.5",
    "gatsby-remark-images": "3.3.10",
    "gatsby-remark-relative-images": "0.3.0",
    "gatsby-source-filesystem": "2.3.11",
    "gatsby-transformer-remark": "2.8.15",
    "gatsby-transformer-sharp": "2.5.5",
    "http-proxy-middleware": "1.0.4",
    "identity-obj-proxy": "3.0.0",
    "intersection-observer": "^0.10.0",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "0.11.5",
    "netlify-cms-app": "2.10.1",
    "netlify-cms-backend-fs": "0.4.4",
    "p-map": "4.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-ripples": "2.2.1",
    "react-spring": "8.0.27",
    "react-typography": "0.16.19",
    "reactn": "2.2.7",
    "remark": "12.0.0",
    "source-map-support": "0.5.19",
    "strip-markdown": "3.1.2",
    "styled-components": "5.1.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.5",
    "typography": "0.16.19",
    "typography-theme-alton": "0.16.19",
    "use-onclickoutside": "0.3.1",
    "uuid": "8.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.15.2",
    "@graphql-codegen/introspection": "1.15.2",
    "@graphql-codegen/typescript": "1.15.2",
    "@graphql-codegen/typescript-operations": "1.15.2",
    "@testing-library/cypress": "6.0.0",
    "@types/graphql": "14.5.0",
    "@types/jest": "26.0.0",
    "@types/lodash": "4.14.155",
    "@types/node": "14.0.13",
    "@types/react": "16.9.36",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.0.0",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "babel-jest": "26.0.1",
    "cypress": "4.8.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-cypress": "2.11.1",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "nodemon": "2.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1",
    "start-server-and-test": "1.11.0",
    "ts-jest": "26.1.0"
  }
}