{
  "name": "recept",
  "description": "Isabels och Henriks receptsamling",
  "version": "1.0.0",
  "author": "Henrik Sommerfeld",
  "keywords": [
    "gatsby",
    "food",
    "recipe"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "gatsby clean",
    "start": "GATSBY_SCREENSHOT_PLACEHOLDER=true nodemon --exec \"gatsby develop --host=0.0.0.0\"",
    "build": "gatsby build",
    "rebuild": "gatsby clean && gatsby build",
    "develop": "gatsby develop",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "serve": "gatsby serve --host=0.0.0.0",
    "test:unit": "jest",
    "test:unit:watch": "jest --watchAll --verbose",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:cypress:ci": "start-server-and-test develop http://localhost:8000 test:cypress",
    "cy": "npm run clean && start-server-and-test develop http://localhost:8000 test:cypress:open",
    "cy:cli": "npm run clean && start-server-and-test develop http://localhost:8000 test:cypress",
    "codegen": "graphql-codegen --config graphql-codegen.yml",
    "upgrade-packages": "yarn upgrade-interactive && npx syncyarnlock"
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*"
    ],
    "delay": "1500"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/henriksommerfeld/food.git"
  },
  "resolutions": {
    "cypress": "4.12.1"
  },
  "dependencies": {
    "@babel/core": "7.11.1",
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "2.4.2",
    "@raae/gatsby-remark-oembed": "0.1.1",
    "babel-plugin-styled-components": "1.11.1",
    "babel-preset-gatsby": "0.5.5",
    "gatsby": "2.24.47",
    "gatsby-background-image": "1.1.1",
    "gatsby-image": "2.4.16",
    "gatsby-plugin-catch-links": "2.3.11",
    "gatsby-plugin-manifest": "2.4.23",
    "gatsby-plugin-netlify": "2.3.13",
    "gatsby-plugin-netlify-cache": "1.2.0",
    "gatsby-plugin-netlify-cms": "4.3.2",
    "gatsby-plugin-offline": "3.2.23",
    "gatsby-plugin-prefetch-google-fonts": "1.4.3",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sharp": "2.6.27",
    "gatsby-plugin-styled-components": "3.3.10",
    "gatsby-plugin-typescript": "2.4.18",
    "gatsby-plugin-typography": "2.5.10",
    "gatsby-remark-copy-linked-files": "2.3.12",
    "gatsby-remark-images": "3.3.25",
    "gatsby-remark-relative-images": "0.3.0",
    "gatsby-source-filesystem": "2.3.24",
    "gatsby-transformer-remark": "2.8.28",
    "gatsby-transformer-sharp": "2.5.13",
    "http-proxy-middleware": "1.0.5",
    "identity-obj-proxy": "3.0.0",
    "intersection-observer": "^0.11.0",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.20",
    "lodash-webpack-plugin": "0.11.5",
    "netlify-cms-app": "2.10.1",
    "netlify-cms-backend-fs": "0.4.4",
    "p-map": "4.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-ripples": "2.2.1",
    "react-spring": "8.0.27",
    "react-typography": "0.16.19",
    "reactn": "2.2.7",
    "remark": "12.0.1",
    "source-map-support": "0.5.19",
    "strip-markdown": "3.1.2",
    "styled-components": "5.1.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.7",
    "typography": "0.16.19",
    "typography-theme-alton": "0.16.19",
    "use-onclickoutside": "0.3.1",
    "uuid": "8.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.8",
    "@graphql-codegen/introspection": "1.17.8",
    "@graphql-codegen/typescript": "1.17.8",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@testing-library/cypress": "6.0.0",
    "@types/graphql": "14.5.0",
    "@types/jest": "26.0.9",
    "@types/lodash": "4.14.159",
    "@types/node": "14.0.27",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "babel-jest": "26.3.0",
    "cypress": "4.12.1",
    "jest": "26.4.0",
    "nodemon": "2.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1",
    "start-server-and-test": "1.11.3",
    "ts-jest": "26.2.0"
  }
}
